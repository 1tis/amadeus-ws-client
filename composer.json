  "keywords": ["amadeus", "GDS", "webservice", "soap", "wsdl", "client", "xml"],
  "minimum-stability": "stable",
  "license": "Apache",
  "authors": [
    {
      "name": "Dieter Devlieghere",
      "email": "dieter.devlieghere@benelux.amadeus.com"
    }
  ],
  "require": {
    "php": ">=5.4.11",
    "psr/log": "1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Amadeus\\": "src/Amadeus"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\Amadeus\\": "tests/Amadeus"
    }
  }
}
